{
  "name": "chrome-extension-es6-import",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --environment happy-dom --reporter=dot",
    "test:watch": "vitest --environment happy-dom",
    "test:e2e:install": "playwright install --with-deps chromium",
    "test:e2e": "node scripts/load-env.js && cross-env NODE_ENV=test playwright test --config=e2e/playwright.config.ts --pass-with-no-tests --reporter=line",
    "test:e2e:ui": "playwright test --config=e2e/playwright.config.ts --ui --timeout 0",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "screenshots": "bash scripts/generate-screenshots.sh",
    "screenshots:clean": "rm -rf screenshots/output promo",
    "icons": "node scripts/generate-icons.js",
    "benchmark": "vitest bench --run --reporter=verbose"
  },
  "dependencies": {
    "@types/html-to-text": "^9.0.4",
    "dompurify": "^3.2.5",
    "html-to-text": "^9.0.5",
    "js-tiktoken": "^1.0.19",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sass": "^1.89.2",
    "sharp": "^0.34.3",
    "uuid": "^11.1.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@crxjs/vite-plugin": "^2.2.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "^0.1.27",
    "@types/dompurify": "^3.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/turndown": "^5.0.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.2.0",
    "cross-env": "^10.1.0",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.3",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "fs-extra": "^11.3.2",
    "happy-dom": "^17.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.0",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "uglify-js": "^3.19.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.3"
  }
}
